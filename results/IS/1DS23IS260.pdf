			 if(usn!=''){
 		    var url = 'http://14.99.184.178:8080/birt/frameset?__report=mydsi/exam/Exam_Result_Sheet_dsce.rptdesign&__format=pdf&USN='+usn;
			window.open(url, '_blank');
			document.getElementById('rp_name').value='';
